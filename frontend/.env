# Disable HMR websocket connection overlay to reduce startup time
VITE_DISABLE_WS_OVERLAY=true

# Skip type checking during development for faster builds
VITE_SKIP_TS_CHECK=true

# Disable React Dev Tools to improve performance
VITE_DISABLE_REACT_DEVTOOLS=true

# Use the browser's full-refresh mode instead of precise HMR
VITE_FULL_REFRESH=true 